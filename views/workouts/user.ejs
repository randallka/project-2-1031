<%- include('../partials/header') %>
<div class="container text-center">
    <h1><%= user.name %> 's Workouts</h1>
</div>
<div class="container px-4 text-center">
    <div class="row gx-5">
      <div class="col">
       <div class="p-3 border bg-light">Created</div>
       <ul class="list-unstyled">
        <% workouts.forEach(workout => { %>
            <li><a href="<%= workout._id %> "><%= workout.name %> </a> <%= workout.createdAt.toLocaleDateString() %> </li>
        <% }) %>
        </ul>
      </div>
      <div class="col">
        <div class="p-3 border bg-light">Liked
        </div>
      </div>
    </div>
  </div>
<%- include('../partials/footer') %>